    "@types/sinon": "^7.0.12",
    "@types/stats-lite": "^2.2.0",
    "@types/string-template": "^1.0.2",
    "azure-devops-node-api": "^10.0.0",
    "azure-pipelines-task-lib": "^2.8.0",
    "js2xmlparser": "^4.0.0",
    "nodemailer": "^6.3.0",
    "performance-now": "^2.1.0",
    "reflect-metadata": "^0.1.13",
    "stats-lite": "^2.0.0",
    "ts-mockito": "^2.3.1",
    "xslt-processor": "^0.11.5"
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@types/node": "^12.0.4",
    "@types/q": "^1.5.2",
    "jest": "^24.8.0",
    "rimraf": "^2.6.3",
    "sinon": "^7.3.2",
    "source-map-loader": "^0.2.3",
    "tfx-cli": "^0.7.8",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.3"
  },
